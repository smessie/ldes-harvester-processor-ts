@prefix rdfc: <https://w3id.org/rdf-connect#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix js: <https://w3id.org/conn/js#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <https://w3id.org/conn#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.

rdfc:LdesHarvester rdfc:jsImplementationOf rdfc:Processor;
    rdfs:label "LDES Harvester";
    rdfs:comment "Processor that can harvest a specific subset of data from a Linked Data Event Stream (LDES).";
    rdfc:file <./lib/index.js>;
    rdfc:class "LdesHarvester";
    rdfc:entrypoint <./>.

[ ] a sh:NodeShape;
    sh:targetClass rdfc:LdesHarvester;
    sh:property [
        sh:class rdfc:Writer;
        sh:path rdfc:writer;
        sh:name "writer";
        sh:maxCount 1;
        sh:minCount 1;
    ], [
        sh:datatype xsd:string;
        sh:path rdfc:url;
        sh:name "url";
        sh:maxCount 1;
        sh:minCount 1;
    ], [
        sh:datatype xsd:dateTime;
        sh:path rdfc:start;
        sh:name "start";
        sh:minCount 1;
        sh:maxCount 1;
    ], [
        sh:datatype xsd:dateTime;
        sh:path rdfc:end;
        sh:name "end";
        sh:minCount 1;
        sh:maxCount 1;
    ], [
        sh:datatype xsd:integer;
        sh:path rdfc:interval;
        sh:name "interval";
        sh:minCount 0;
        sh:maxCount 1;
    ], [
        sh:datatype xsd:integer;
        sh:path rdfc:amountPerInterval;
        sh:name "amountPerInterval";
        sh:minCount 0;
        sh:maxCount 1;
    ], [
        sh:datatype xsd:boolean;
        sh:path rdfc:urlIsView;
        sh:name "urlIsView";
        sh:minCount 0;
        sh:maxCount 1;
    ].
